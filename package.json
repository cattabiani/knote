{
  "name": "knote",
  "version": "0.0.1",
  "description": "A Quasar/Vue app to take organized notes if a folder-like structure.",
  "productName": "knote",
  "author": "Katta <alessandro.cattabiani@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "deploy": "quasar build -m pwa && gh-pages -d dist/pwa",
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^4.1.3",
    "quasar": "^2.16.0",
    "uuid": "^13.0.0",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.9.0",
    "@quasar/cli": "^2.4.1",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.0.1",
    "prettier": "^2.8.8",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^0.34.6",
    "workbox-build": "7.0.x",
    "workbox-cacheable-response": "7.0.x",
    "workbox-core": "7.0.x",
    "workbox-expiration": "7.0.x",
    "workbox-precaching": "7.0.x",
    "workbox-routing": "7.0.x",
    "workbox-strategies": "7.0.x"
  },
  "engines": {
    "node": "^18",
    "npm": ">=6.13.4"
  }
}
